<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<ResourceDictionary.ThemeDictionaries>
		<ResourceDictionary x:Key="Light">
			<SolidColorBrush
				x:Key="NavigationViewTopPaneBackground"
				Color="#a7b9f1" />
			<SolidColorBrush
				x:Key="NavigationViewSelectionIndicatorForeground"
				Color="{StaticResource TextFillColorTertiary}" />
			<SolidColorBrush
				x:Key="CommandBarBackground"
				Color="#7b97ea" />
			<SolidColorBrush
				x:Key="PageItemsBorder"
				Color="Gray" />
		</ResourceDictionary>
		<ResourceDictionary x:Key="Dark">
			<SolidColorBrush
				x:Key="NavigationViewTopPaneBackground"
				Color="#1f49c7" />
			<SolidColorBrush
				x:Key="NavigationViewSelectionIndicatorForeground"
				Color="{StaticResource TextFillColorTertiary}" />
			<SolidColorBrush
				x:Key="CommandBarBackground"
				Color="#18399a" />
			<SolidColorBrush
				x:Key="PageItemsBorder"
				Color="DimGray" />
		</ResourceDictionary>
	</ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
