<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<ResourceDictionary.ThemeDictionaries>
		<ResourceDictionary x:Key="Light">
			<SolidColorBrush
				x:Key="NavigationViewTopPaneBackground"
				Color="#8db0f2" />
			<SolidColorBrush
				x:Key="CommandBarBackground"
				Color="#9bbaf3" />
			<SolidColorBrush
				x:Key="PageItemsBorder"
				Color="Gray" />
		</ResourceDictionary>
		<ResourceDictionary x:Key="Dark">
			<SolidColorBrush
				x:Key="NavigationViewTopPaneBackground"
				Color="#103a89" />
			<SolidColorBrush
				x:Key="CommandBarBackground"
				Color="#0e3377" />
			<SolidColorBrush
				x:Key="PageItemsBorder"
				Color="DimGray" />
		</ResourceDictionary>
	</ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
